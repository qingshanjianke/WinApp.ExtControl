<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    po:Freeze="True">

	<!--  字体颜色  -->
	<Color x:Key="WE.PrimaryTextColor" po:Freeze="True">#303133</Color>
	<SolidColorBrush x:Key="WE.PrimaryTextBrush" po:Freeze="True" Color="{StaticResource WE.PrimaryTextColor}" />

	<Color x:Key="WE.RegularTextColor" po:Freeze="True">#606266</Color>
	<SolidColorBrush x:Key="WE.RegularTextBrush" po:Freeze="True" Color="{StaticResource WE.RegularTextColor}" />

	<!--  背景色  -->
	<Color x:Key="WE.BackgroundColor" po:Freeze="True">#FFFFFF</Color>
	<SolidColorBrush x:Key="WE.BackgroundBrush" po:Freeze="True" Color="{StaticResource WE.BackgroundColor}" />

	<Color x:Key="WE.WindowBorderColor" po:Freeze="True">#409EFF</Color>
	<SolidColorBrush x:Key="WE.WindowBorderBrush" po:Freeze="True" Color="{StaticResource WE.WindowBorderColor}" />

	<SolidColorBrush x:Key="WE.WindowTextBrush" po:Freeze="True" Color="{StaticResource WE.BackgroundColor}" />

	<Color x:Key="WE.BaseColor" po:Freeze="True">#DCDFE6</Color>
	<SolidColorBrush x:Key="WE.BaseBrush" po:Freeze="True" Color="{StaticResource WE.BaseColor}" />

	<Color x:Key="WE.BaseMoveColor" po:Freeze="True">#F5F7FA</Color>
	<SolidColorBrush x:Key="WE.BaseMoveBrush" po:Freeze="True" Color="{StaticResource WE.BaseMoveColor}" />

	<Color x:Key="WE.LighterColor" po:Freeze="True">#EBEEF5</Color>
	<SolidColorBrush x:Key="WE.LighterBrush" po:Freeze="True" Color="{StaticResource WE.LighterColor}" />

	<Color x:Key="WE.LightColor" po:Freeze="True">#E4E7ED</Color>
	<SolidColorBrush x:Key="WE.LightBrush" po:Freeze="True" Color="{StaticResource WE.LightColor}" />

	<Color x:Key="WE.ChartXAxisColor" po:Freeze="True">#E0E6F1</Color>
	<SolidColorBrush x:Key="WE.ChartXAxisBrush" po:Freeze="True" Color="{StaticResource WE.ChartXAxisColor}" />

	<Color x:Key="WE.ChartFillColor" po:Freeze="True">#6E7079</Color>
	<SolidColorBrush x:Key="WE.ChartFillBrush" po:Freeze="True" Color="{StaticResource WE.ChartFillColor}" />

</ResourceDictionary>